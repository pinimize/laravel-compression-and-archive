{
    "name": "pinimize/laravel-compression-and-archive",
    "description": "A Laravel package for compression and archiving",
    "keywords" : [
        "pinimize",
        "laravel",
        "compression"
    ],
    "homepage" : "https://github.com/pinimize/laravel-compression-and-archive",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Einar Hansen",
            "email": "einar@einarhansen.dev",
            "homepage": "https://einarhansen.dev"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.5",
        "laravel/pint": "^1.17",
        "orchestra/testbench": "^9.1.5",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^10.5|^11.0",
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Pinimize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pinimize\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pinimize\\PinimizeServiceProvider"
            ]
        }
    },
    "scripts" : {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config" : {
        "sort-packages" : true,
        "allow-plugins" : {
            "phpstan/extension-installer" : true,
            "pestphp/pest-plugin" : true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
